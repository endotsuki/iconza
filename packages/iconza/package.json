{
  "name": "iconza",
  "version": "1.0.39",
  "private": false,
  "description": "A modern, accessible icon set with original brand colors, full TypeScript support, and delightful animations.",
  "homepage": "https://iconza.vercel.app",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "icons",
    "react",
    "typescript",
    "svg",
    "icon-library",
    "icon-set",
    "accessible-icons",
    "animated-icons",
    "brand-icons"
  ],
  "scripts": {
    "start": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts",
    "prepublishOnly": "pnpm build"
  },
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "license": "MIT",
  "vite": {
    "resolve": {
      "dedupe": [
        "react",
        "react-dom"
      ]
    }
  }
}
